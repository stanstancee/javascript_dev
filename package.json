{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {

    "prestart": "babel-node buildScript/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": " babel-node buildScript/srcServer.js",
    "poststart": "babel-node buildScript/postMessage.js",
    "lint":"esw webpack.config.* src buildScript --color",
    "lint:watch":"npm run lint -- --watch",
    "test": "mocha --reporter progress buildScript/testSetup.js \"src/**/*.test.js\"",
    "test:watch":"npm run test -- --watch",
    "localtunnel": "lt --port 3000 --subdomain stancee",
    "share": "npm-run-all --parallel prestart open:src poststart localtunnel "
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",

    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.17.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-watch": "2.1.14",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.15.0",
    "localtunnel": "^1.9.2",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",

    "surge": "0.20.4",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
